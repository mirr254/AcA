<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_about_us_menu"
        tools:context="com.maos.aca.ui.AboutUsMenuActivity">

    <com.mindorks.placeholderview.ExpandablePlaceHolderView
            android:id="@+id/expandableView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scrollbars="vertical" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0" tools:layout_editor_absoluteY="17dp"
            tools:layout_editor_absoluteX="4dp"/>

</android.support.constraint.ConstraintLayout>
